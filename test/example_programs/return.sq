
string strTable(int index){
    if(index == 0){
        return "abc";
    }
    else if(index == 1){
        return "def";
    }
    else if(index == 2){
        string value = "ghi";
        return value;
    }
    else{
        return "other";
    }
}

void printCount(){
    for(int i=0; i < 5; ++i){
        print(i);
        if(i==4){
            return;
        }
    }
}
int main(){
    for(int i=0; i < 4; ++i){
        print(strTable(i));
    }
    printCount();
    return 0;
}