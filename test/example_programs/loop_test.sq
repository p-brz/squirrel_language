int main(){
    int counter = 0;
    for(int i=0; i < 10; ++i){
        counter++;
    }
    print("counter == 10? ", counter == 10);
    
    while(counter < 12){
        print("Isto deve ser executado 2 vezes!");
        ++counter;
    }
    
    do{
        print("Isto deve ser executado 3 vezes!");
    }while(counter < 15);
    
    return 0;
}
